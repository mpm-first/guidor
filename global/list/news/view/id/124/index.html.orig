<script>
    function changeStoreWidget(value) {
        var result = value.split(";;");
        if(result.length == 2) {
            var location = result[0];
            var storeCode = result[1];
        }
        if(storeCode != 'global') {
            if (location == "http://us.guidor.com/" || location == "http://guidor.ca/") {
                window.open(location);
            } else {
                window.location.href=location;
            }
        } else {
            setCookie();
        }
    }
</script>

