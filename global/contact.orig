<script>
    function changeStoreWidget(value) {
        var result = value.split(";;");
        if(result.length == 2) {
            var location = result[0];
            var storeCode = result[1];
        }
        if(storeCode != 'global') {
            if (location == "http://us.guidor.com/" || location == "http://guidor.ca/") {
                window.open(location);
            } else {
                window.location.href=location;
            }
        } else {
            setCookie();
        }
    }
</script>

<script type="text/javascript">
    jQuery(document).ready(function () {

        if (Mage.Cookies.get('is_professional') == null) {
            jQuery(".modal-prof").show();
            jQuery("body").addClass("modal-open");
        }
    });

    function setCookie()
    {
        Mage.Cookies.set('is_professional', '1', new Date(new Date().getTime() + (24 * 60 * 60 * 1000)));
        jQuery(".modal-prof").hide();
        jQuery("body").removeClass("modal-open");
    }

    function addhttp(url)
    {
        if (!/^(f|ht)tps?:\/\//i.test(url)) {
            url = "http://" + url;
        }
        return url;
    }


</script>
